  "license": "MIT",
  "description": "A simple DND library for web",
  "homepage": "https://github.com/eunjin11/ddo-dnd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eunjin11/ddo-dnd.git"
  },
  "keywords": [
    "react",
    "typescript",
    "dnd",
    "draganddrop"
  ],
  "scripts": {
